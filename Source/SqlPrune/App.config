<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
    </startup>
    <appSettings>
        <!-- AWS profile name or location http://docs.aws.amazon.com/AWSSdkDocsNET/latest/DeveloperGuide/net-dg-config-creds.html -->
        <add key="AWSProfileName" value="default"/>
        <!-- AWS profile location, leave empty to load from the current user folder (e.g. ~\.aws.\config) -->
        <add key="AWSProfilesLocation" value=""/>
        <!-- Default region -->
        <add key="AWSDefaultRegionEndPoint" value="eu-west-1"/>
    </appSettings>
</configuration>
