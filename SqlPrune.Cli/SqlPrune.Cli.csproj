<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Comsec.SqlPrune</RootNamespace>
    <AssemblyName>sqlprune</AssemblyName>
    <OutputType>Exe</OutputType>
    <RuntimeIdentifier>win10-x64</RuntimeIdentifier>
    <StartupObject>Comsec.SqlPrune.Program</StartupObject>
    <Company>Comsec Investigations Ltd</Company>
    <Authors>Comsec</Authors>
    <Description>CLI executable to prune files from a directory of S3 bucket.</Description>
    <Copyright>Comsec Investigations Ltd</Copyright>
    <Version>0.4.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.CommandLine" Version="2.0.0-beta1.20303.1" />
    <PackageReference Include="LightInject" Version="6.3.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SqlPrune.Core\SqlPrune.Core.csproj" />
  </ItemGroup>
  
</Project>